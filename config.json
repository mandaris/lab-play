{
  "params": {
      "themeVersion": "1.8.02",
      "author": {
        "name": "Mandaris",
        "email": "info@MandarisMoore.com",
        "avatar": "https://micro.blog/mandaris/avatar.jpg",
        "username": "mandaris",
        "description": "A High-quality stranger. I strive to be a good husband, father, and friend. This blog is my way of expressing myself.",
        "search_space_include_replies": true,
        "search_space_include_transcripts": true
      }

  },
  "markup": {
    "highlight": {
      "noClasses": false
    },
    "tableOfContents": {
      "endLevel": 5,
      "startLevel": 3
    },
    "goldmark": {
      "parser": {
        "wrapStandAloneImageWithinParagraph": false,
        "attribute": {
          "block": true
        }
      },
      "extensions": {
        "passthrough": {
          "delimiters": {
            "block": [
              [
                "\\[",
                "\\]"
              ],
              [
                "$$",
                "$$"
              ]
            ],
            "inline": [
              [
                "\\(",
                "\\)"
              ]
            ]
          },
          "enable": true
        }
      }
    }
  }
}
